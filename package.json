{
  "name": "digital-clock-vue",
  "version": "1.0.3",
  "description": "Ô∏èA digital clock simulation build on Vue.",
  "author": "Zkerhcy<czhao5956@gmail.com>",
  "license": "MIT",
  "keywords": [
    "clock",
    "digital",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zkerhcy/digital-clock-vue.git"
  },
  "main": "dist/digital-clock-vue.ssr.js",
  "browser": "dist/digital-clock-vue.esm.js",
  "module": "dist/digital-clock-vue.esm.js",
  "unpkg": "dist/digital-clock-vue.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/serve-dev.*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/serve-dev.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "prepublishOnly": "npm run build",
  "postpublish" : "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-replace": "^2.2.1",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "cross-env": "^6.0.3",
    "less-loader": "^5.0.0",
    "minimist": "^1.2.0",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.5",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  }
}